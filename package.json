    "babel-plugin-config-module-resolver": "^1.0.2",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
